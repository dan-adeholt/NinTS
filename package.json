{
  "name": "nints",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "lint": "yarn tsc --noEmit && eslint . --ext .ts,.tsx,.js --max-warnings=0",
    "perfTest": "yarn esbuild src/emulator/perfTest.ts --platform=node --bundle --outfile=/tmp/out.js && node /tmp/out.js",
    "profile": "yarn perfTest && node --prof --no-logfile-per-isolate /tmp/out.js && node --prof-process --preprocess v8.log > v8.json"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@types/react-transition-group": "^4.4.5",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "classnames": "^2.2.6",
    "eslint": "^8.26.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.31.10",
    "idb": "^7.1.1",
    "js-sha1": "^0.6.0",
    "pngjs": "^6.0.0",
    "pre-commit": "^1.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-transition-group": "^4.4.5",
    "wavefile": "^11.0.0"
  },
  "devDependencies": {
    "@types/pngjs": "^6.0.1",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.1.0",
    "typescript": "^4.6.4",
    "vite": "^4.1.1",
    "vitest": "^0.29.3"
  },
  "pre-commit": [
    "test",
    "lint"
  ]
}
